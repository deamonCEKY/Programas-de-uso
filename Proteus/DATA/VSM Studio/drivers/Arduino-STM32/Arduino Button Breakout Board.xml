<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<CLIP CATEGORY="Breakout Peripherals" FILE="Arduino-STM32/Arduino Button Breakout Board.pdsclip" FAMILY="Arduino-STM32" DESC="Momentary Action Push Button">
   <PERIPHERAL TYPE="Arduino Button" ELEMENT="BTN1" >
      <!-- Hardware Interface -->
      <INTERFACE OBJECT="GenericButton">
         <PIN NAME="IO" TYPE="DIGITAL">
            <TERMINAL ELEMENT="PA5"/>
         </PIN>
      </INTERFACE>

      <!-- Firmware Driver -->
      <DRIVER HEADER="Generic.h">
         <FUNCTION DESC="Button State">
            <RETURN TYPE="BOOLEAN"/>
         </FUNCTION>
      </DRIVER>
   </PERIPHERAL>
</CLIP>

